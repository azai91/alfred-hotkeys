				<key>applescript</key>
				<string>on alfred_script(q)
  set is_running to true
  if application "Sublime Text" is running then
    set app_name to "Sublime Text"
    set app_name_escaped to "Sublime\\ Text"
  else if application "Android Studio" is running then
    set app_name to "Android Studio"
    set app_name_escaped to "Android\\ Studio"
  else if application "Xcode" is running then
    set app_name to "Xcode"
    set app_name_escaped to "Xcode"
  else
    set is_running to false
	do shell script "open -a Sublime\\ Text"
  end if

  if is_running then
    tell application "System Events"      if frontmost of process app_name then        set visible of process app_name to false
      else
        do shell script "open -a " &amp; app_name_escaped	  end if    end tell
  end if  
end alfred_script</string>
